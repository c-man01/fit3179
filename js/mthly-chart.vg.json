{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 500,
  "height": 400,
  "title": "London Bicyle Hire 2019",
  "data": {"url": "https://raw.githubusercontent.com/c-man01/fit3179/master/data/mth-hires.csv"},
  "mark": "area",
  "encoding": {
    "x": {
      "timeUnit": "month", 
      "field": "date",
      "type": "temporal",
      "title": "Month",
      "axis": {"format": "%b"}
    },
    "y": {
      "field": "hire-cnt",
      "type": "quantitative",
      "axis": {"tickCount": 8},
      "title": "Monthly Hire Totals"
    },
      "color":{
        "field": "Borough",
        "scale": {"scheme": "paired"}
      }
    }
  }