{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 500,
    "height": 300,
    "layer": [
      {
        "data": {
          "url": "https://raw.githubusercontent.com/vega/vega-datasets/next/data/londonBoroughs.json",
          "format": {
            "type": "topojson",
            "feature": "boroughs"
          }
        },
        "projection": {
          "type": "conicEqualArea"
        },
        "mark": {
          "type": "geoshape",
          "fill": "lightgray",
          "stroke": "white"
        }
      },
      {
        "data": {
          "url": "https://raw.githubusercontent.com/c-man01/fit3179/master/DV2/data/cycle-stations.csv"
        },
        "projection": {
          "type": "conicEqualArea"
        },
        "mark": "circle",
        "encoding": {
          "longitude": {
            "field": "Lng",
            "type": "quantitative"
          },
          "latitude": {
            "field": "Lat",
            "type": "quantitative"
          },
          "size": {"value": 10},
          "color": {"value": "steelblue"}
        }
      }
    ]
  }